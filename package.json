{
  "name": "qr-code-maker",
  "version": "1.0.0",
  "description": "A customizable QR code generator with image upload functionality.",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "node src/app.js",
    "build:web": "vite build",
    "build": "echo 'Build step not defined'",
    "build:container": "podman build -t qr-code-maker .",
